
<!-- ANCHOR: Admin Navbar Content  -->

<div class="row admin-content-head bg-dark">
    <div class="col-sm-2">
        <div class="d-flex align-items-center admin-content-head-administrator">
            <a href="/administrator/account">
                <button type="submit" class="btn btn-dark btn-sm"><i class="fas fa-user-shield"></i>
                    Administrator
                </button>
            </a>
        </div>
    </div>
    <div class="col-sm-6 text-center admin-head-search">
        <form action="/administrator/dashboard" method="GET" class="form-inline">
            <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search" name="search">
            <button class="btn btn-secondary btn-sm my-2 my-sm-0" type="submit"><i class="fas fa-search"></i></button>
        </form>
    </div>
    <div class="col-sm-4 ">
        <div class="admin-head-icon">
            
            <a href="/administrator/dashboard" class="btn btn-dark btn-sm" style="border:1px solid #000000">

                <svg class="mb-1" width="1.25em" height="1.25em" viewBox="0 0 16 16" class="bi bi-house-door-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <path d="M6.5 10.995V14.5a.5.5 0 0 1-.5.5H2a.5.5 0 0 1-.5-.5v-7a.5.5 0 0 1 .146-.354l6-6a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 .146.354v7a.5.5 0 0 1-.5.5h-4a.5.5 0 0 1-.5-.5V11c0-.25-.25-.5-.5-.5H7c-.25 0-.5.25-.5.495z"/>
                    <path fill-rule="evenodd" d="M13 2.5V6l-2-2V2.5a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5z"/>
                  </svg>

            </a>

            <button class="btn btn-dark btn-sm" id="shorthNoticeBtn"><i class="fas fa-bell"></i></button>
            <div class="dropdown-menu" id="shorhNoticeToggle">

                <div class="setting-account text-danger">
                    <svg width="1.25em" height="1.25em" viewBox="0 0 16 16" class="bi bi-bell-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path d="M8 16a2 2 0 0 0 2-2H6a2 2 0 0 0 2 2zm.995-14.901a1 1 0 1 0-1.99 0A5.002 5.002 0 0 0 3 6c0 1.098-.5 6-2 7h14c-1.5-1-2-5.902-2-7 0-2.42-1.72-4.44-4.005-4.901z"/>
                      </svg>
                    Notice
                </div>
                <form action="/administrator/dashboard/notice" method="POST">
                <div class="setting-application p-1">
                    
                        <textarea class="form-control" name="notice" id="exampleFormControlTextarea1" rows="8"></textarea>
                  
                </div>
                <div class="text-center px-2">
                    <button type="submit" class="btn btn-sm btn-primary w-100">Submit</button>
            
                </div>
                </form>
                
            </div>
            

            <button class="btn btn-dark btn-sm"><a href="/administrator/emails"><i class="fas fa-envelope"></i></a></button>
            <button class="btn btn-dark btn-sm setting"><i class="fas fa-cog"></i></button>
            <div class="dropdown-menu  setting-menu">

            <div class="setting-account">
                <span>Signed as</span>
                <br>
                <span class="account-name px-2"><%=data.name?data.name:'Administrator'%></span>
            </div>
            
            <div class="setting-application">
                    <a href="/administrator/account">
                        <div class="setting-item">Your Profile</div>
                    </a>
                    <a href="/auth/change-password""><div class=" setting-item">Change Password</div></a>
                <a href="/administrator/security-password">
                    <div class="setting-item">Security Password</div>
                </a>
            </div>

            <div class="setting-application">
                <a href="/administrator/create">
                    <div class="setting-item">Administrator</div>
                </a>
                <a href="/administrator/developer/info">
                    <div class="setting-item">Help</div>
                </a>

                <a href="/administrator/setting">
                    <div class="setting-item">Setting</div>
                </a>

                <a href="/auth/logout">
                    <div class="setting-item">Singout</div>
                </a>
            </div>
        </div>
        <form action="/auth/logout" method="GET">
            <button class="btn btn-dark btn-sm" type="submit"><i class="fas fa-sign-out-alt"></i>
                LOGOUT
            </button>
        </form>
    </div>
</div>
</div>