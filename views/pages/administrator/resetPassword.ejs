<%-include('../../partials/admin/adminHead.ejs')%>
<div class="container"><%-include('../../partials/Error/alert.ejs')%></div>
<div class="container">
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6 mt-5">

            <div class="card mt-5">
                <h1 class="text-center card-title pt-3">JASA</h1>
                <div class="card-body px-5 pb-5 ">
                    <form action="<%=url%>" method="POST">
                        <div class="form-group">
                            <label for="exampleInputEmail1">RESET YOUR PASSWORD</label>
                            <input type="text" class="form-control <%=error.password?'is-invalid':''%>" placeholder="Enter Your Password"
                                id="exampleInputEmail1" name="password">
                            <div class="invalid-feedback">
                                <%=error.password%>
                            </div>

                            <label for="exampleInputEmail1">CONFIRM PASSWORD</label>
                            <input type="text" class="form-control <%=error.confirmPassword?'is-invalid':''%>" placeholder="Enter Your Confirm Password"
                                id="exampleInputEmail1" name="confirmPassword">
                            <div class="invalid-feedback">
                                <%=error.confirmPassword%>
                            </div>
                            


                            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone
                                else.</small>
                        </div>
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </form>
                </div>
            </div>
        </div>
        <div class="col-sm-3"></div>
    </div>
</div>
<!-- Bootstrap JS Links -->
<%-include('../../partials/admin/adminFooter.ejs')%>

<body>

    </html>