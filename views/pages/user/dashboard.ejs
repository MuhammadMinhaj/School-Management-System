<!-- User For Open Toggler class ( toggled ) past on page-wrapper -->
<div class="page-wrapper chiller-theme"> 
  <a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
    <i class="fas fa-bars"></i>
  </a>

  <!-- Navbar -->
  <%-include('../../partials/user/navbar.ejs')%>
  <!-- Navbar End -->

  <!-- sidebar-wrapper  -->
  <main class="page-content">
    <div class="container-fluid">
      <h2 style="font-weight:600;color: #545b62;">DASHBOARD</h2>
     <hr>
   
         <div class="row custom_user_card_container">
              <div class="col-sm-3">
                <div class="user_card">
                  <h6>Exam</h6>
                    <h5><%=ExaminationCount<10?'0'+ExaminationCount:ExaminationCount%></h5>
                </div>
                
                

              </div>
              <div class="col-sm-3">
                <div class="user_card">
                  <h6>Students</h6>
                  <h5><%=totalStudents<10?'0'+totalStudents:totalStudents%></h5>
                </div>
                
              
              </div>
              <div class="col-sm-3">
                <div class="user_card">
                    <h6>Class</h6>
                    <h5><%=classes.length<10?'0'+classes.length:classes.length%></h5>
                </div>
              </div>
              <div class="col-sm-3">
                <div class="user_card">
                  <h6>Results</h6>
                  <h5><%=totalResults<10?'0'+totalResults:totalResults%></h5>
                </div>
              </div>
          </div>

  

          <div class="row mt-2">
            <div class="col-sm-7">
                <div class="row user_left_card px-3">
                  <div class="col-sm-4 my-3">
                    <a href="/user/results/published/status" class="user_small_card">
                      <div class="user_card_icon">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-circle-square" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                          <path d="M0 6a6 6 0 1 1 12 0A6 6 0 0 1 0 6z"/>
                          <path d="M12.93 5h1.57a.5.5 0 0 1 .5.5v9a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5v-1.57a6.953 6.953 0 0 1-1-.22v1.79A1.5 1.5 0 0 0 5.5 16h9a1.5 1.5 0 0 0 1.5-1.5v-9A1.5 1.5 0 0 0 14.5 4h-1.79c.097.324.17.658.22 1z"/>
                        </svg>
                        </div>
                 
                        <div class="user_card_text">Status</div>
                      </a>
                
                  </div>   

                  <div class="col-sm-4 my-3">
                    <a href="/user/class/create" class="user_small_card">
                      <div class="user_card_icon">
                          <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-house-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                            <path fill-rule="evenodd" d="M8 3.293l6 6V13.5a1.5 1.5 0 0 1-1.5 1.5h-9A1.5 1.5 0 0 1 2 13.5V9.293l6-6zm5-.793V6l-2-2V2.5a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5z"/>
                            <path fill-rule="evenodd" d="M7.293 1.5a1 1 0 0 1 1.414 0l6.647 6.646a.5.5 0 0 1-.708.708L8 2.207 1.354 8.854a.5.5 0 1 1-.708-.708L7.293 1.5z"/>
                          </svg>
                      </div>
                 
                        <div class="user_card_text">CLASS</div>
                    </a>
                
                  </div>



                  <div class="col-sm-4 my-3">
                    <div class="modal fade" id="classModal" tabindex="-1" role="dialog">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title">Class</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                              <span aria-hidden="true">&times;</span>
                            </button>
                          </div>
                          <div class="modal-body">
                            <ul class="list-group ">
                             
                             
                             <%for(let c of classes){%>
                                <li class="list-group-item user-list-group-item"><a href="/user/class/update/<%=c._id%>" class="user_item_links">Add Subject In Class <%=c.name%></a></li>
                              <%}%>
                              
                            </ul>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            
                          </div>
                        </div>
                      </div>
                    </div>
                    
                    <div class="user_small_card">
                      <div class="user_card_icon"  data-toggle="modal" data-target="#classModal">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-collection-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                          <path d="M0 13a1.5 1.5 0 0 0 1.5 1.5h13A1.5 1.5 0 0 0 16 13V6a1.5 1.5 0 0 0-1.5-1.5h-13A1.5 1.5 0 0 0 0 6v7z"/>
                          <path fill-rule="evenodd" d="M2 3a.5.5 0 0 0 .5.5h11a.5.5 0 0 0 0-1h-11A.5.5 0 0 0 2 3zm2-2a.5.5 0 0 0 .5.5h7a.5.5 0 0 0 0-1h-7A.5.5 0 0 0 4 1z"/>
                        </svg>
                        </div>
                 
                        <div class="user_card_text">Subjects</div>
                    </div>
                
                  </div>
                  
                 

                  <div class="col-sm-4 my-3">
                    <div class="modal fade" id="examModal" tabindex="-1" role="dialog">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title">Create Examination</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                              <span aria-hidden="true">&times;</span>
                            </button>
                          </div>
                          <div class="modal-body">
                            <ul class="list-group ">
                             
                             
                              <%for(let c of classes){%>
                                 <li class="list-group-item user-list-group-item"><a href="/user/class/exam/create/<%=c._id%>" class="user_item_links">Create Examination Of Class <%=c.name%> </a></li>
                               <%}%>
                               
                             </ul>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="user_small_card" data-toggle="modal" data-target="#examModal">
                      <div class="user_card_icon">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-stickies-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                          <path fill-rule="evenodd" d="M0 1.5A1.5 1.5 0 0 1 1.5 0H13a1 1 0 0 1 1 1H1.5a.5.5 0 0 0-.5.5V14a1 1 0 0 1-1-1V1.5z"/>
                          <path fill-rule="evenodd" d="M3.5 2A1.5 1.5 0 0 0 2 3.5v11A1.5 1.5 0 0 0 3.5 16h6.086a1.5 1.5 0 0 0 1.06-.44l4.915-4.914A1.5 1.5 0 0 0 16 9.586V3.5A1.5 1.5 0 0 0 14.5 2h-11zm6 8.5v4.396c0 .223.27.335.427.177l5.146-5.146a.25.25 0 0 0-.177-.427H10.5a1 1 0 0 0-1 1z"/>
                        </svg>
                        </div>
                 
                        <div class="user_card_text">Exam</div>
                    </div>
                
                  </div>
                  <div class="col-sm-4 my-3">
                    <div class="modal fade" id="resultsModal" tabindex="-1" role="dialog">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title">Results Management</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                              <span aria-hidden="true">&times;</span>
                            </button>
                          </div>
                          <div class="modal-body">
                            <ul class="list-group ">
                             
                             
                              <%for(let c of classes){%>
                                 <li class="list-group-item user-list-group-item"><a href="/user/results/create/<%=c._id%>" class="user_item_links">Cretae Results Of Class <%=c.name%> </a></li>
                               <%}%>
                               
                             </ul>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                          
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="user_small_card" data-toggle="modal" data-target="#resultsModal">
                      <div class="user_card_icon">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-info-circle-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                          <path fill-rule="evenodd" d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm.93-9.412l-2.29.287-.082.38.45.083c.294.07.352.176.288.469l-.738 3.468c-.194.897.105 1.319.808 1.319.545 0 1.178-.252 1.465-.598l.088-.416c-.2.176-.492.246-.686.246-.275 0-.375-.193-.304-.533L8.93 6.588zM8 5.5a1 1 0 1 0 0-2 1 1 0 0 0 0 2z"/>
                        </svg>
                        </div>
                 
                        <div class="user_card_text">Results</div>
                    </div>
                
                  </div>
                  <div class="col-sm-4 my-3">
                    <div class="modal fade" id="profileModal" tabindex="-1" role="dialog">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title">Registered Students</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                              <span aria-hidden="true">&times;</span>
                            </button>
                          </div>
                          <div class="modal-body">
                            <ul class="list-group ">
                             
                             
                              <%for(let c of classes){%>
                                 <li class="list-group-item user-list-group-item"><a href="/user/student/create/<%=c._id%>" class="user_item_links">Registered Student On Class ( <%=c.name%> ) </a></li>
                               <%}%>
                               
                             </ul>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="user_small_card" data-toggle="modal" data-target="#profileModal">
                      <div class="user_card_icon">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-person-plus-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                          <path fill-rule="evenodd" d="M1 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H1zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6zm7.5-3a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1 0-1H13V5.5a.5.5 0 0 1 .5-.5z"/>
                          <path fill-rule="evenodd" d="M13 7.5a.5.5 0 0 1 .5-.5h2a.5.5 0 0 1 0 1H14v1.5a.5.5 0 0 1-1 0v-2z"/>
                        </svg>
                        </div>
                 
                        <div class="user_card_text">Students</div>
                    </div>
                
                  </div>
                  
                  

                </div>
          
            </div>
            <div class="col-sm-5 removeXPadding addUserMarginTop">
              <div class="user_right_card">
                  
              
                      <h5 class="user_notice_title">
                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-bell-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                          <path d="M8 16a2 2 0 0 0 2-2H6a2 2 0 0 0 2 2zm.995-14.901a1 1 0 1 0-1.99 0A5.002 5.002 0 0 0 3 6c0 1.098-.5 6-2 7h14c-1.5-1-2-5.902-2-7 0-2.42-1.72-4.44-4.005-4.901z"/>
                        </svg>
                        Notice
                      </h5>
                    
                    <div class="user_notice_body">
                      
                        <%if(webModel.notice){%>
                          
                            <%=webModel.notice%>
                          
                        <%}else{%>
                            <h1 style="font-weight: 600;">WELCOME</h1>
                        <%}%>
                        
                      
                    </div>
                
              </div>
            </div>
          </div>


    </div>
  </main>
</div>




<%-include('../../partials/user/footer.ejs')%>

    
